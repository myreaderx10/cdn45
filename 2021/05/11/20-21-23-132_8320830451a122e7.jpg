/* CSS PARA LAS NUEVAS FICHAS */

.fichaContainer {
display: flex;
font-weight: 500;
font-size: 12px;
margin-bottom: 20px;
flex-direction: column;
width: 100%;
}

.fichaContainer .portada {
text-align: center;
margin: 0;
overflow: hidden;
}

.fichaContainer .portada  img {
width: fit-content;
height: 100%;
object-fit: cover;
}

.fichaContainer h4, .fichaContainer h3 {
  border-color: lightgray; 
  border-style: solid;
  border-width: 0 0 1px;
  margin-bottom: 5px !important;
  margin-top: 25px !important;
}

.fichaContainer .zonaCompra ul {
  display: flex;
  flex-flow: row wrap;
  list-style: none !important; 
  margin: 0px !important;
  justify-content: flex-start;
}

.fichaContainer .zonaCompra li {
  margin-left: 0px !important;
  padding: 0px !important;
  margin-right: 5px;
}

.fichaContainer .zonaCompra img {
  width:50px;
  height:50px;
  border-radius: 10px;
}

.fichaContainer .zonaCompra div {
  width: 50px;
  height: 50px;
  font-size: 10px;
  background-color: #e03688;
  color: white;
  font-weight: bold;
  line-height: 15px;
  display: flex;
  justify-content: center;
  flex-direction: column;
  text-align: center;
  border-radius: 10px;
}

.fichaContainer .principal .principal__sinopsis {
font-size: 12px; 
padding-right: 2px;
}

.fichaContainer .principal .principal__generos {
font-size: 14px;
