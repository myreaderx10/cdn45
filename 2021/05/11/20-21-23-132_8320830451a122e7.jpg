height: 45px;
line-height: 15px;
}

.fichaContainer .edicionesContainer {
display: grid;
grid-template-columns: repeat(3,auto);
grid-row-gap: 10px;
grid-column-gap: 10px;
margin-top: 10px;
}

.fichaContainer .staffContainer {
    display: grid;
    grid-row-gap: 5px;
}


.fichaContainer .staffCard {
  height: 60px; 
  width: 100%;
}

.fichaContainer .staffCardContainer {
  display: inline-grid;
  grid-template-columns: 40px auto;
  grid-column-gap: 5px;
  height: 100%;
}

.fichaContainer .staffCardAvatar {
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: cover;
}

.fichaContainer .staffCardInfo {
  font-size: 16px;
  align-self: center;
}

.fichaContainer .enlaces__data {
