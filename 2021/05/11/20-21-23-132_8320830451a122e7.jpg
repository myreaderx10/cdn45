/* CSS FICHAS DE TEMPORADA DE ANIME */

.card__list {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-auto-rows: 375px;
  grid-column-gap: 16px;
  grid-row-gap: 16px;
  font-size: 14px;
}
.card__list .card {
  overflow: hidden;
  height: 375px;
  box-sizing: border-box;
  width: auto !important;
  float: none !important;
}
.card__list .card .card__header {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 12px;
  background: #F0F0F0;
  box-sizing: border-box;
  
}
.card__list .card .card__header .card__title {
  text-decoration: none;
  color: #e03688;
  font-weight: 700;
  font-size: 18px;
  margin-bottom: 0px;
  transition: opacity 0.2s ease-in;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
  width: 100%;
}
.card__list .card .card__header .card__title:hover {
  opacity: 0.7;
}
.card__list .card .card__header .tags {
  display: flex;
  justify-content: center;
  width: 100%;
  list-style: none !important;
  margin: 0 !important;
}
.card__list .card .card__header .tags li {
  padding-right: 12px;
  padding: 0 6px !important;
  margin-left: 0 !important;
}
.card__list .card .card__container {
  display: flex;
  background: #FFFFFF;
  box-sizing: border-box;
  height: 304px;
}
.card__list .card .card__container figure {
  width: 40%;
  height: 100%;
  margin: 0;
  overflow: hidden;
}
.card__list .card .card__container figure img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.card__list .card .card__container .info {
  width: 60%;
  display: flex;
  flex-direction: column;
}
.card__list .card .card__container .info .info__data {
  list-style: none !important;
  margin: 0 !important;
  display: flex;
  flex-wrap: wrap;
  border-style: solid;
  border-color: lightgrey;
  border-width: 1px 0 0 1px;
  height: 105px;
  font-size: 13px !important;
}
.card__list .card .card__container .info .info__data li {
  flex: 1 0 50%;
  border-style: solid;
  border-color: lightgrey;
  border-width: 0 1px 1px 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 6px !important;
  margin: 0 !important;
  overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis;
}
.card__list .card .card__container .info .info__data li a {
  text-decoration: none;
  color: #ad7c58;
  transition: color 0.2s ease-in;
  text-align: center;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.card__list .card .card__container .info .info__data li a:hover {
  color: #e03688; 
}
.card__list .card .card__container .info .info__synopsis {
  overflow-y: scroll;
  overflow-x: hidden;
  font-size: 13px;
  margin: 6px 0 6px 12px !important;
  line-height: 20px;
  height: 222px;
}
.card__list .card .card__container .info .info__links {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 48px;
  list-style: none !important;
}
.card__list .card .card__container .info .info__links li {
  padding: 0 6px !important;
  margin-left: 0 !important;
}

