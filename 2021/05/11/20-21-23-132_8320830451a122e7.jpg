grid-template-columns: repeat(1,100%);
}

.fichaContainer .reviewCard {
height: 70px;
}

.fichaContainer .reviewCardContainer {
display: inline-grid;
grid-template-columns: 70px minmax(100px,300px);
grid-column-gap: 10px;
